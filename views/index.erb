<% if session["user_id"]  != nil %>
  <p>logged in
    <%=session["user_id"]%>
  </p>
<% else %>
  <p>not logged in</p>
<% end%>
<ol class = "links">
  <% @articles.each do |article| %>
    <li>
      <p class="#"> <img class = "arrow" src = "grayarrow.gif">
        <%= article[:title] %>
      </p>
      <p class="#">
        <a href= <%= "#{article[:url]}" %> > <%= article[:url] %> </a>
      </p>
      <br />
      <p class="#">
        <%= article[:description] %>
      </p>
    </li>
  <% end %>
</ol>
<p>test</p>
